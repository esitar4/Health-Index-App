@page "/search"

<label>Search</label>
<div class="container-fluid mb-5">
    <div class="row">
        <div class="col-9">
            <input type="text" />
        </div>
        <div class="col-3">
            <button @onclick="SearchFood">Submit</button>
        </div>
    </div>
</div>
*
*@if (json is not null)
{
    foreach (var food in json.Foods.Food) {
        <h4>@food.Food_Name (@food.Food_Id)</h4>
        <br />
    }
} else{
    <h4>Something went very wrong</h4>
}